class Book:
    def __init__(self, bookId, title, author):
        self.bookId = bookId
        self.title = title
        self.author = author

    def __repr__(self):
        return f"Book(ID: {self.bookId}, Title: '{self.title}', Author: '{self.author}')"

def linear_search(books, target_title):
    """
    Perform a linear search to find a book by title.
    
    :param books: List of Book objects
    :param target_title: The title of the book to search for
    :return: The Book object if found, otherwise None
    """
    for book in books:
        if book.title.lower() == target_title.lower():
            return book
    return None

def binary_search(books, target_title):
    """
    Perform a binary search to find a book by title. Assumes the list is sorted.
    
    :param books: List of Book objects (sorted by title)
    :param target_title: The title of the book to search for
    :return: The Book object if found, otherwise None
    """
    low, high = 0, len(books) - 1
    while low <= high:
        mid = (low + high) // 2
        mid_title = books[mid].title.lower()
        if mid_title == target_title.lower():
            return books[mid]
        elif mid_title < target_title.lower():
            low = mid + 1
        else:
            high = mid - 1
    return None

# Example usage
if __name__ == "__main__":
    # Create a list of books
    books = [
        Book(1, "The Great Gatsby", "F. Scott Fitzgerald"),
        Book(2, "To Kill a Mockingbird", "Harper Lee"),
        Book(3, "1984", "George Orwell"),
        Book(4, "Pride and Prejudice", "Jane Austen"),
        Book(5, "The Catcher in the Rye", "J.D. Salinger")
    ]
    
    # Perform linear search
    title_to_search = "1984"
    found_book_linear = linear_search(books, title_to_search)
    print(f"Linear Search: {found_book_linear}")
    
    # Sort books by title (necessary for binary search)
    books.sort(key=lambda book: book.title.lower())
    
    # Perform binary search
    found_book_binary = binary_search(books, title_to_search)
    print(f"Binary Search: {found_book_binary}")
